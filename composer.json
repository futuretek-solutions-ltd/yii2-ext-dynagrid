{
    "name": "futuretek/yii2-dynagrid",
    "description": "FTS DynaGrid",
    "keywords": [
        "yii2",
        "extension",
        "widget",
        "dynamic",
        "grid",
        "columns",
        "order",
        "sort",
        "visibility",
        "hide",
        "show",
        "reorder"
    ],
    "type": "yii2-extension",
    "license": "Apache",
    "homepage": "http://www.futuretek.cz/",
    "authors": [
        {
            "name": "Lukas Cerny",
            "email": "lukas.cerny@futuretek.cz"
        }
    ],
    "require": {
        "kartik-v/yii2-dynagrid": "^1.4.8"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "futuretek\\dynagrid\\": ""
        }
    },
    "extra": {
        "yii-config": {
            "modules": {
                "gridview": {
                    "class": "\\kartik\\grid\\Module"
                },
                "dynagrid": {
                    "class": "\\kartik\\dynagrid\\Module",
                    "dbSettings": {
                        "tableName": "dynagrid"
                    },
                    "dbSettingsDtl": {
                        "tableName": "dynagrid_dtl"
                    }
                }
            }
        }
    }
}
